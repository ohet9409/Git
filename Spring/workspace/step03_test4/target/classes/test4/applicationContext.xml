<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id = "sungjuk" class="test4.SungjukDTO">
	<constructor-arg name="name" value="홍길동"/>
	<constructor-arg name="kor" value="97"/>
	<constructor-arg name="eng" value="100"/>
	<constructor-arg name="math" value="95"/>
	</bean>
	<bean id = "impl" class="test4.SungjukImpl">
	<constructor-arg name="dto" ref="sungjuk"/>
	</bean>
</beans>
